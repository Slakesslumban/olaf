Q1>

echo Enter a number 
read number
fact=1 
for (( i = 1; i <= number; i++ ))
do
fact=$((fact*i))
done 
echo Factorial of $number is $fact  


  Q2> 

echo"Enter a number :" 
read num
sum=0;
while [$num-gt0] 
do 
digit=$((num%10)) 
sum=$((sum+digit)) 
num=$((num/10))
done 
echo"sum of digit is$sum"   


  Q3>

echo"Enter the first number :"
read num
sum=0;
 if [$num-gt0];then 
echo"The number is positive"
elif[$num-lt0];then
echo"The number is negative" 
else 
echo"The number is zero"
fi   


